datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model User {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  profile_img String
  email   String   @unique
  name    String?
  level   Int 
  password String
}

model Place {
  id     String  @id @default(auto()) @map("_id") @db.ObjectId
  description    String
  acessLevel    Int
}